<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.swmandroid">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SWMAndroid"
        tools:targetApi="31">
        <activity
            android:name=".screen.fullservice.FullServiceActivity"
            android:exported="false" />
        <activity
            android:name=".screen.mypage.MyPageActivity"
            android:exported="false" />
        <activity
            android:name=".screen.community.CommunityActivity"
            android:exported="false" />
        <activity
            android:name=".screen.test.TestActivity"
            android:exported="false" />
        <activity
            android:name=".screen.easylearning.EasyLearningActivity"
            android:exported="false" />
        <activity
            android:name=".screen.main.MainActivity"
            android:exported="false" />
        <activity
            android:name=".screen.login.LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.MySplashScreen.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>